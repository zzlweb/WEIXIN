/* miniprogram/pages/start/index.wxss */

.container{
    width: 100%;
    height: 100%;
    position: absolute;
}

.bg{
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    bottom: 0;
    z-index: -1;
}

.go{
    position: absolute;
    right: 30rpx;
    top: 150rpx;
    z-index: 9;
}

.go button {
    font-size: 28rpx;
    letter-spacing: 4rpx;
    border-radius: 30rpx;
    color: #000;
    background: rgba(255, 255, 255, 0.781);
     display: flex;
    justify-content: center;
    align-items: center;
    text-align: center;
    width: 160rpx;
    height: 60rpx;
}